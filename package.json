{
  "name": "lacona",
  "version": "0.8.2",
  "description": "Natural Language Parsing Framework",
  "main": "lib/lacona.js",
  "scripts": {
    "test": "npm run test:node && npm run test:phantom",
    "lint": "npm run lint:lib && npm run lint:test",
    "lint:lib": "jshint --config node_modules/lacona-util-jshint/lib.json --reporter node_modules/jshint-stylish/stylish.js lib",
    "lint:test": "jshint --config node_modules/lacona-util-jshint/test.json --reporter node_modules/jshint-stylish/stylish.js test",
    "test:phantom": "npm run make && npm run make:test && gulp phantom",
    "test:node": "mocha test --reporter dot",
    "validate": "npm run lint && npm run test",
    "cover": "istanbul cover _mocha test -- -R spec",
    "make": "mkdir dist || true && browserify lib/lacona.js --standalone lacona --outfile dist/lacona.js && npm run uglify",
    "make:test": "mkdir tmp || true && browserify test/*.js --ignore lacona --outfile tmp/tests.js",
    "uglify": "uglifyjs --output dist/lacona.min.js dist/lacona.js ",
    "clean": "gulp clean",
    "prepublish": "npm run make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona.git"
  },
  "keywords": [
    "lingustic",
    "natural",
    "language",
    "parsing"
  ],
  "author": {
    "name": "Brandon Horst",
    "email": "brandonhorst@gmail.com",
    "url": "http://github.com/brandonhorst"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona",
  "devDependencies": {
    "browserify": "^7.0.0",
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "del": "^1.1.0",
    "glob": "^4.3.1",
    "gulp": "^3.8.10",
    "gulp-filesize": "0.0.6",
    "gulp-mocha-phantomjs": "^0.5.1",
    "gulp-rename": "^1.2.0",
    "gulp-streamify": "0.0.5",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.10",
    "jshint-stylish": "^1.0.0",
    "lacona-util-jshint": "^0.1.0",
    "mocha": "^2.0.1",
    "sinon": "^1.12.1",
    "sinon-chai": "^2.6.0",
    "uglify-js": "^2.4.16",
    "vinyl-source-stream": "^1.0.0"
  },
  "dependencies": {
    "async-each": "^0.1.6",
    "async-each-series": "^0.1.1",
    "inherits": "^2.0.1",
    "semver": "^4.1.0"
  }
}
