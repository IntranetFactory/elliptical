{
  "name": "lacona",
  "version": "0.18.1",
  "description": "Natural Language Parsing Framework",
  "main": "lib/lacona.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel/register",
    "build": "babel src --out-dir lib",
    "validate": "npm run build && npm test",
    "clean": "rimraf lib coverage",
    "cover": "istanbul cover _mocha test -- -R dot --compilers jsx:babel/register && istanbul report text",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona.git"
  },
  "keywords": [
    "lingustic",
    "natural",
    "language",
    "parsing"
  ],
  "author": {
    "name": "Brandon Horst",
    "email": "brandonhorst@gmail.com",
    "url": "http://github.com/brandonhorst"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona",
  "devDependencies": {
    "babel": "^4.4.5",
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "event-stream": "^3.2.2",
    "istanbul": "^0.3.5",
    "lacona-util-fulltext": "^0.2.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.2.8",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0"
  },
  "dependencies": {
    "async-each": "^0.1.6",
    "lodash": "^3.0.0"
  }
}
